# RustDesk
RUSTDESK_PATH=./rustdesk-server/target/release
RUSTDESK_KEY=_  # Auto-keygen; change for prod (e.g., your-secret-key)
RUSTDESK_PORTS=21115-21117  # ID server (UDP/TCP; firewall if needed)
RELAY_PORTS=21116-21119  # Relay for NAT fallback

# Backend/DB
MONGO_URL=mongodb://localhost:27017/barrieros  # Local Mongo in Termux
DB_NAME=barrieros
SECRET_KEY=your-super-secret-jwt-key-for-did-auth-change-this
MONGO_PORT=27017  # If custom

# Android Paths (for emulators)
ANDROID_SDK=/data/data/com.termux/files/usr/opt/android-sdk  # Or ~/android-sdk
UPLOAD_DIR=./uploads  # Relative to server.py
JWT_SECRET=super-secret-baked-key-change-per-build  # Hardcoded in JS
SERVER_PORT=8080  # Internal localhost
MOCK_DEVICE_RAM=4096  # Fixed for 6GB
UPLOAD_DIR=  # Use RNFS.CachesDirectoryPath + '/uploads'
